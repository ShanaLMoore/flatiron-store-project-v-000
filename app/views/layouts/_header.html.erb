<nav class="">
    <ul>
      <%= link_to "Store Home", store_path %><br>
      <% if !!current_user %>
        Signed in as <%= current_user.email %><br>
        <%= link_to "Sign out", '/logout' %><br>   
      <% else %>
        <%= link_to "Sign up", '/signup' %> or <%= link_to "sign in", '/login' %><br>
      <% end %>

      <% if !!current_user && !!current_cart %>  
